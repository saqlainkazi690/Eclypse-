{
  "name": "eclypse-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "eclipse-fashion",
    "server"
  ],
  "scripts": {
    "start": "cd eclipse-fashion && npm start",
    "build": "cd eclipse-fashion && npm run build",
    "server": "cd server && npm run dev",
    "dev": "concurrently \"npm run start\" \"npm run server\""
  },
  "devDependencies": {
    "concurrently": "^8.0.1"
  }
} 